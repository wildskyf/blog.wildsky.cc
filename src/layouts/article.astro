---
import PageLayout from './page.astro'
import Article from '@/components/Article.astro'

const { frontmatter, compiledContent } = Astro.props
const { title, feature_image } = frontmatter

const lang = 'tw'
const path_prefix = lang === 'en' ? '/en' : ''

---

<PageLayout
  lang={lang}
  pageName={title}
  pageDesc={frontmatter.excerpt}
>
  <Article
    feature_image_url={feature_image}
    title={title}
    content={compiledContent()}
  />
</PageLayout>
